{
  "name": "@echosky/board-lexicons",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "files": [
    "lexicons/",
    "dist/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "ts-node --esm scripts/build-types.ts && ts-node --esm scripts/validate.ts",
    "validate": "ts-node --esm scripts/validate.ts",
    "diff": "ts-node --esm scripts/diff-compat.ts",
    "docs": "ts-node --esm scripts/generate-docs.ts",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@atproto/lexicon": "^0.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
